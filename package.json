{
  "name": "demonas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --noStackTrace --notify --detectOpenHandles",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:ios": "xcodebuild -workspace ios/demonas.xcworkspace -scheme demonas -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build > /dev/null",
    "pretest:e2e": "detox build -c ios.sim",
    "test:e2e": "detox test -c ios.sim --cleanup",
    "generate": "plop"
  },
  "dependencies": {
    "@react-native-firebase/admob": "^6.2.0",
    "@react-native-firebase/analytics": "^6.2.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-native-firebase/auth": "^6.2.0",
    "@react-native-firebase/crashlytics": "^6.2.0",
    "@react-native-firebase/database": "^6.2.0",
    "@react-native-firebase/firestore": "^6.2.0",
    "@react-native-firebase/functions": "^6.2.0",
    "@react-native-firebase/perf": "^6.2.0",
    "@react-native-firebase/storage": "^6.2.0",
    "native-base": "^2.13.8",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-svg": "^11.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jasmine": "^3.5.2",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/prettier": "^1.19.0",
    "@types/react-native": "^0.61.9",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "detox": "^15.1.4",
    "enzyme": "^3.11.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.12.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/demonas.app",
        "build": "npm run build:ios",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
