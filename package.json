{
  "name": "native-base",
  "description": "Essential cross-platform UI components for React Native",
  "keywords": [
    "android",
    "ios",
    "react",
    "native",
    "react-native",
    "native-modules",
    "react-component",
    "navbar",
    "packager"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekyAnts/NativeBase.git"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/NativeBase"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --noStackTrace --notify --detectOpenHandles",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "generate": "plop",
    "compile": "rm -rf dist/* && tsc -p ./",
    "transpile": "npm run compile && cp src/basic/Icon/NBIcons.json dist/src/basic/Icon",
    "prepublish": "npm run transpile",
    "postinstall": "node addEjectScript.js",
    "build:js": "babel src --out-dir dist --ignore node_modules,dist,jest.config.js,src/__mocks__,src/__tests__ --source-maps --extensions \".ts,.tsx\"",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@react-native-firebase/admob": "^6.2.0",
    "@react-native-firebase/analytics": "^6.2.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-native-firebase/auth": "^6.2.0",
    "@react-native-firebase/crashlytics": "^6.2.0",
    "@react-native-firebase/database": "^6.2.0",
    "@react-native-firebase/firestore": "^6.2.0",
    "@react-native-firebase/functions": "^6.2.0",
    "@react-native-firebase/perf": "^6.2.0",
    "@react-native-firebase/storage": "^6.2.0",
    "native-base": "^2.13.8",
    "native-base-shoutem-theme": "^0.3.1",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-svg": "^11.0.1",
    "react-navigation": "^4.1.0",
    "react-navigation-stack": "^2.0.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/blueimp-md5": "^2.7.0",
    "@types/color": "^3.0.1",
    "@types/create-react-class": "^15.6.3",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jasmine": "^3.5.2",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/prettier": "^1.19.0",
    "@types/react-native": "^0.61.9",
    "@types/react-native-drawer": "^2.5.2",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.2.1",
    "detox": "^15.1.4",
    "enzyme": "^3.11.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.12.0",
    "ts-jest": "^25.1.0",
    "tsc-silent": "^1.2.1",
    "typescript": "^3.8.0-beta"
  },
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-native": ">=0.56.0"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/demonas.app",
        "build": "npm run build:ios",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
